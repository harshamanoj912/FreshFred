<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/singleOrderShowForRiders"
    android:layout_width="match_parent"
    android:layout_height="180dp"
    android:padding="16dp">

    <TextView
        android:id="@+id/ordersListAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/addressTXTOrderQueue"
        app:layout_constraintTop_toBottomOf="@+id/ordersListOrderId"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/ordersListMobileNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/customerMobileTXTOrderQueue"
        app:layout_constraintTop_toBottomOf="@+id/ordersListCustomerName"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/ordersListCustomerName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/customerNameTXTOrderQueue"
        app:layout_constraintTop_toBottomOf="@+id/ordersListPrice"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/ordersListOrderId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/orderIDTXTOrderQueue"
        app:layout_constraintTop_toBottomOf="@+id/ordersListMobileNumber"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/ordersListPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/foodPriceTXTOrderQueue"
        app:layout_constraintTop_toBottomOf="@+id/ordersListFoodName"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/ordersListFoodName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/foodNameTXTOrderQueue"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0" />

    <TextView
        android:id="@+id/foodNameTXTOrderQueue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Food Name : "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/foodPriceTXTOrderQueue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total Price : "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/foodNameTXTOrderQueue"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/addressTXTOrderQueue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address : "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orderIDTXTOrderQueue"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/orderIDTXTOrderQueue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Order ID : "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/customerMobileTXTOrderQueue"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/customerMobileTXTOrderQueue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Customer Mobile : "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/customerNameTXTOrderQueue"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/customerNameTXTOrderQueue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Customer Name : "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/foodPriceTXTOrderQueue"
        app:layout_constraintVertical_bias="0.03" />

    <Button
        android:id="@+id/button3"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:text="Shipped"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.873"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.473" />

</androidx.constraintlayout.widget.ConstraintLayout>